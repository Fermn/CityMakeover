<%= simple_form_for @fixpoint do |f| %>
  <%= f.input :fixed, as: :hidden, value: 'false' %>
  <%= f.simple_fields_for :fixpoint_attachments do |p| %>
    <%= p.label :photos %>
    <%= p.file_field :url, multiple: true, name: "fixpoint_attachments[photo][]" %>
  <% end %>
  <%= f.submit %>
<% end %>